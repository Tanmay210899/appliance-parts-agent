# LLM Configuration
LLM_PROVIDER=google  # google, openai, anthropic
LLM_MODEL=gemini-2.0-flash-exp
GOOGLE_API_KEY=your_google_api_key_here

# Embedding Configuration (Local SBERT)
EMBEDDING_MODEL=all-MiniLM-L6-v2  # Fast and efficient
# Alternative models: all-mpnet-base-v2 (more accurate), paraphrase-multilingual-MiniLM-L12-v2

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=partselect
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=partselect_embeddings

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
